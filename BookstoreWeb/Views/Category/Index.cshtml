@model List<Category>

<h1>Category Lists</h1>
<table class = "table table-bordered table-striped">
    <thead>
        <tr>
        <th>
            Category Name
        </th>
        <th>
            Display Order
        </th>
        </tr>
    </thead>
    @foreach(var obj in Model)
    {
    <tbody>
        <tr>
            <td>
                @obj.Name
            </td>
            <td>
                @obj.DisplayOrder
            </td>
        </tr>
    </tbody>
    }
</table>